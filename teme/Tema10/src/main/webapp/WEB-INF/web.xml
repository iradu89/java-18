<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>customServlet</servlet-name>
    <servlet-class>CustomServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>emptyServlet</servlet-name>
    <servlet-class>EmptyServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>customServlet</servlet-name>
    <url-pattern>/superCustom/servlet/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>emptyServlet</servlet-name>
    <url-pattern>/superCustom/emptyServlet/*</url-pattern>
  </servlet-mapping>

</web-app>
